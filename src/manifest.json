{
    "name": "Rubik's Cube Timer",
    "description": "This app allows you to record and manage your cube records",
    "version": "0.1.1",
    "manifest_version": 2,

    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    },
    "icons": {
        "128": "img/icon-128.png"
    },
    "permissions": [
        "storage",
        "https://apis.google.com/"
    ]
}
